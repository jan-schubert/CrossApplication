<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:light1="clr-namespace:CrossApplication.Core.Net.Themes.LightBlue;assembly=CrossApplication.Core.Net.Themes"
                    xmlns:themes="clr-namespace:CrossApplication.Core.Wpf.Themes">
    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type themes:Colors}, ResourceId=TextColorKey}"
        Color="{x:Static light1:Colors.TextColor}" />
    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type themes:Colors}, ResourceId=HintTextColorKey}"
        Color="{x:Static light1:Colors.HintTextColor}" />
    <SolidColorBrush
        x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type themes:Colors}, ResourceId=NavigationBarBackgroundColorKey}"
        Color="{x:Static light1:Colors.NavigationBarBackgroundColor}" />
</ResourceDictionary>