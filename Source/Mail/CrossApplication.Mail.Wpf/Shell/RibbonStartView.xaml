<fluent:RibbonTabItem x:Class="CrossApplication.Mail.Wpf.Shell.RibbonStartView"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                      xmlns:fluent="urn:fluent-ribbon"
                      xmlns:shell="clr-namespace:CrossApplication.Mail.Wpf.Shell"
                      xmlns:viewModels1="clr-namespace:CrossApplication.Core.Wpf.Common.ViewModels;assembly=CrossApplication.Core.Wpf.Common"
                      xmlns:properties1="clr-namespace:CrossApplication.Core.Wpf.Common.Properties;assembly=CrossApplication.Core.Wpf.Common"
                      mc:Ignorable="d" Header="{x:Static properties1:Resources.ShellRibbonTabStart}"
                      viewModels1:ViewModelLocator.AutoWireViewModel="{x:Type shell:RibbonStartViewModel}">
    <fluent:RibbonGroupBox Header="{x:Static properties1:Resources.ShellRibbonTabStartGroupNew}">
        <fluent:Button Header="{x:Static properties1:Resources.ShellRibbonTabStartGroupNewNewMail}" />
    </fluent:RibbonGroupBox>
</fluent:RibbonTabItem>