<fluent:RibbonTabItem x:Class="CrossApplication.Mail.Wpf.Shell.RibbonStartView"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                      xmlns:fluent="urn:fluent-ribbon"
                      xmlns:properties="clr-namespace:CrossApplication.Wpf.Common.Properties;assembly=CrossApplication.Wpf.Common"
                      xmlns:viewModels="clr-namespace:CrossApplication.Wpf.Common.ViewModels;assembly=CrossApplication.Wpf.Common"
                      xmlns:shell="clr-namespace:CrossApplication.Mail.Wpf.Shell"
                      mc:Ignorable="d" Header="{x:Static properties:Resources.ShellRibbonTabStart}"
                      viewModels:ViewModelLocator.AutoWireViewModel="{x:Type shell:RibbonStartViewModel}">
    <fluent:RibbonGroupBox Header="{x:Static properties:Resources.ShellRibbonTabStartGroupNew}">
        <fluent:Button Header="{x:Static properties:Resources.ShellRibbonTabStartGroupNewNewMail}" />
    </fluent:RibbonGroupBox>
</fluent:RibbonTabItem>